{
  "name": "application_evaluator",
  "version": "0.1.0",
  "private": true,
  "description": "UI for the FVH Application Evaluator application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=default",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome ci .",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:production": "npm audit --omit=dev --audit-level=moderate"
  },
  "author": "Forum Virium Helsinki",
  "dependencies": {
    "@sentry/react": "^8.0.0",
    "bootstrap": "^5.3.3",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-jsonschema-form": "^1.8.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.1.1",
    "react-svg-radar-chart": "^1.2.0",
    "reactstrap": "^9.2.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@sentry/vite-plugin": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^25.0.1",
    "sass": "^1.82.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
