{
  "name": "application_evaluator",
  "version": "0.1.0",
  "private": true,
  "description": "UI for the FVH Application Evaluator application",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=default",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome ci .",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:production": "npm audit --omit=dev --audit-level=moderate"
  },
  "author": "Forum Virium Helsinki",
  "dependencies": {
    "@sentry/react": "^8.0.0",
    "@types/lodash": "^4.14.168",
    "@vitejs/plugin-react": "^1.1.1",
    "bootstrap": "^4.4.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-jsonschema-form": "^1.8.1",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-svg-radar-chart": "^1.2.0",
    "reactstrap": "^8.9.0",
    "sass": "^1.23.7",
    "typescript": "^3.7.4",
    "vite": "^2.7.0",
    "xlsx": "^0.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@sentry/vite-plugin": "^4.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^4.0.15",
    "jsdom": "^25.0.0",
    "vitest": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
